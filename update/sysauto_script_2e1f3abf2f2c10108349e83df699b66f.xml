        <script><![CDATA[var dueSoon = new GlideRecord('x_58872_needit_needit_task');
var today= new GlideDateTime();

dueSoon.addQuery('due_date','>=', today);
dueSoon.addQuery('due_date','<', gs.hoursAgo(-24));
dueSoon.addQuery('state','!=', '3');
dueSoon.query();

while(dueSoon.next())
	{
		gs.info("Tasks which are due in next 24 hrs from " + today + " = " + dueSoon.number);
		gs.eventQueue('x_58872_needit.dueSoon',dueSoon,dueSoon.number,dueSoon.short_description,'my_queue_due_soon');
	}]]></script>
